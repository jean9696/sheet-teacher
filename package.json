{
  "name": "client-models",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=3012 react-app-rewired start",
    "build": "react-app-rewired build",
    "build:schema": "npx habx-schema",
    "build:types": "apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --globalTypesFile=src/globalTypes.ts --includes=src/**/*.ts --tagName=gql --addTypename types",
    "bundlewatch": "cross-env BUNDLEWATCH=true npm run build && mv build build-bundlewatch && bundlewatch --config .bundlewatch.config.js",
    "analyze": "cross-env ANALYZE=1 npm run build",
    "lint": "eslint \"src/**\"",
    "type:coverage": "tscov",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/client-3d-models.git"
  },
  "author": "Jean Dessane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/client-3d-models/issues"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.4",
    "@habx/apollo-multi-endpoint-link": "^0.1.0",
    "@habx/lib-client-acl": "^1.1.0",
    "@habx/lib-client-reporting": "^1.1.0",
    "@habx/lib-form-helper": "^3.6.1",
    "@habx/ui-core": "^2.32.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "final-form": "^4.19.1",
    "final-form-arrays": "^3.0.2",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "lodash": "^4.17.15",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.4.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "styled-components": "^5.1.0",
    "tone": "^13.8.25",
    "vexflow": "^1.2.93"
  },
  "devDependencies": {
    "@habx/eslint-config-client": "^3.1.0",
    "@habx/eslint-config-graphql": "^1.2.0",
    "@habx/graphql-scripts": "^2.12.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/segment-analytics": "0.0.32",
    "@types/styled-components": "^5.0.1",
    "@types/vexflow": "^1.2.33",
    "bundlewatch": "^0.2.6",
    "circular-dependency-plugin": "^5.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "react-app-rewired": "^2.1.5",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
